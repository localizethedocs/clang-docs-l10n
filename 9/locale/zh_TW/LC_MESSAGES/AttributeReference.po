msgid "global-dynamic"
msgstr ""

#: ../../../AttributeReference.rst:3810
msgid "local-dynamic"
msgstr ""

#: ../../../AttributeReference.rst:3811
msgid "initial-exec"
msgstr ""

#: ../../../AttributeReference.rst:3812
msgid "local-exec"
msgstr ""

#: ../../../AttributeReference.rst:3814
